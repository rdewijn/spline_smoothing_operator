{
  "name": "Spline Smoothing Operator",
  "description": "Perform a smoothing using splines",
  "tags": ["smoothing", "modeling", "splines"],
  "authors": ["rdw"],
  "urls": ["https://github.com/tercen/spline_smoothing_operator"],
  "properties": [
   {
    "kind": "DoubleProperty",
    "name": "n_interpolation",
    "defaultValue": 100,
    "description": "Number of points to sue for interpolated / smoothed line"
    },
    {
    "kind": "DoubleProperty",
    "name": "smooth_par",
    "defaultValue": 0,
    "description": "Smoothing parameter (higher is smoother)"
  },
    {
    "kind": "BooleanProperty",
    "name": "all_knots",
    "defaultValue": true,
    "description": "Use maximum number of knots"
  },
      {
    "kind": "DoubleProperty",
    "name": "n_knots",
    "defaultValue": 10,
    "description": "Number of knots to use when all_knots is false"
  }
  ]
}


